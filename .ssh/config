Host sshmtp
        HostName sshmtp.phys.tue.nl
        User gortali
        IdentityFile ~/.ssh/id_rsa
        ForwardX11Trusted yes
        ForwardX11 yes
        XAuthLocation /opt/X11/bin/xauth
 
Host post-1
     HostName post-1
     User gortali
     ProxyCommand ssh gortali@sshmtp nc %h 22
 
Host post-2
     HostName post-2
     User gortali
     ProxyCommand ssh gortali@sshmtp nc %h 22

Host post-3
     HostName post-3
     User gortali
     ProxyJump post-1

Host volta
     HostName volta
     User gortali
     ProxyJump post-1

Host jacobi
	Hostname jacobi.maths.sissa.it
	User gortali
    IdentityFile ~/.ssh/id_rsa
    ForwardX11Trusted yes
    ForwardX11 yes

Host storage-4
User gortali
Hostname 131.155.0.105
