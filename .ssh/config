ForwardX11Trusted yes
ForwardX11 yes

Host sshmtp
        HostName sshmtp.phys.tue.nl
        User gortali
        PubkeyAcceptedKeyTypes ssh-rsa
        HostKeyAlgorithms ssh-rsa
        IdentityFile ~/.ssh/id_rsa
 
Host post-1
     HostName post-1
     User gortali
     ProxyCommand ssh gortali@sshmtp nc %h 22
 
Host post-2
     HostName post-2
     User gortali
     ProxyCommand ssh gortali@sshmtp nc %h 22

Host post-3
     HostName post-3
     User gortali
     ProxyJump post-1

Host volta
     HostName volta
     User gortali
     ProxyJump post-1

Host ampere
     HostName ampere
     User gortali
     ProxyJump post-1

